{
  "name": "scrapy",
  "version": "0.1.0",
  "description": "Selenium scrapper ",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_PATH=./build&& node build/index.js",
    "build": "tsc -p .",
    "dev": "nodemon -e ts --exec \"npm run debug -- shopify checkout bot1\"",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run lint:fix && npm run prettier:fix",
    "debug": "npm run build && set DEBUG='puppeteer-cluster:*' && set NODE_INSPECT_RESUME_ON_START=1 && set NODE_PATH=./build && node inspect build/index.js"
  },
  "pre-commit": "lint, prettier",
  "author": "Kho, Bryan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "cheerio": "^1.0.0-rc.10",
    "chrome-aws-lambda": "^10.0.0",
    "commander": "^8.0.0",
    "discord.js": "13.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "puppeteer": "^10.4.0",
    "puppeteer-cluster3": "^1.0.0",
    "puppeteer-core": "^10.1.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "sequelize": "^6.6.5",
    "tesseract.js": "^2.1.4",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.2.0",
    "nodemon": "^2.0.9",
    "pre-commit": "^1.2.2",
    "prettier": "2.5.1",
    "sequelize-cli": "^6.2.0",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^10.0.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
