{
  "name": "scrapy",
  "version": "0.1.0",
  "description": "Selenium scrapper ",
  "main": "index.js",
  "scripts": {
    "start": "set NODE_PATH=./build&& node build/index.js",
    "build": "tsc -p .",
    "dev": "nodemon src/index.ts",
    "pre-deploy": "rm -rf node_modules .serverless && npm install && npm run build && ;prepare-layer.sh",
    "deploy-dev": "npm run pre-deploy && set NODE_ENV=dev sls deploy"
  },
  "author": "Kho, Bryan",
  "license": "MIT",
  "dependencies": {
    "commander": "^8.0.0",
    "express": "^4.17.1",
    "selenium": "^2.20.0",
    "serverless-offline": "^7.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "nodemon": "^2.0.9",
    "puppeteer": "^10.1.0",
    "serverless": "^2.51.2",
    "ts-node": "^10.0.0"
  }
}
